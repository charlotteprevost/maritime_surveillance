# Example environment file (NO secrets)
#
# Copy to `.env` and fill in values:
#   cp .env.example .env

# Global Fishing Watch API token (required for backend API calls)
GFW_API_TOKEN=

# Allowed frontend origins for CORS (comma-separated, origins only, no paths)
FRONTEND_ORIGINS=http://localhost:8080

# Allowed backend origins (if used by your deployment setup)
BACKEND_ORIGINS=http://localhost:5000

